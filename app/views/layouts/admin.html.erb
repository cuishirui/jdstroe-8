<!DOCTYPE html>
<html>
  <head>
    <title>Jdstore</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container-fluid">
      <%= render "common/navbar"%>
      <div class="col-md-2">
        <%= link_to("Product", admin_products_path, class:"btn btn-default btn-primary")%>
      </div>
      <div class="col-md-10">
      <%= yield %>
      </div>
    </div>

  </body>
</html>
